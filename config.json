{
  "icons": {
    "src": "_src/assets/icons/*.*",
    "dest": "public/",
    "dist": "docs/game/"
  },
  "html": {
    "src": "_src/templates/*.html",
    "partials": "_src/templates/partials/",
    "dest": "public/",
    "dist": "docs/game/"
  },
  "scss": {
    "src": "_src/assets/scss/**/*.scss",
    "dest": "public/assets/css/",
    "dist": "docs/game/assets/css/"
  },
  "js": {
    "src": [
      "_src/assets/js/libs/*.js",
      "_src/assets/js/battleship/*.js",
      "_src/assets/js/players/*.js"
    ],
    "dest": "public/assets/js/",
    "dist": "docs/game/assets/js/"
  },
  "images": {
    "src": "_src/assets/images/**/*.{gif,jpg,jpeg,JPG,JPEG,png,svg}",
    "dest": "public/assets/images/",
    "dist": "docs/game/assets/images/"
  },
  "watch": {
    "html": "_src/**/*.html"
  },
  "documentation": {
    "src": [
      "DOCUMENTATION.md",
      "./_src/**/*.js"
    ],
    "configFile": "./.jsdoc.json"
  }
}